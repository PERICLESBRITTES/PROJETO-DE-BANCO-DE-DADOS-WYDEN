CREATE TABLE `pesquisa de candidato` (
`NUMERO DE ORDEM` INT NOT NULL AUTO_INCREMENT,
`ANO DA PESQUISA` YEAR NULL DEFAULT NULL,
`NOME DO ENTREVISTADOR` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`LOCAL DA PESQUISA` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`DATA DA PESQUISA` DATE NULL DEFAULT NULL,
`HORA DA PESQUISA` TIME NULL DEFAULT NULL,
`NOME DO ENTREVISTADO` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`SITUAÇÃO SOCIAL` SET('SOLTEIRO','CASADO') NOT NULL DEFAULT 'SOLTEIRO,CASADO' COLLATE 'utf8mb4_0900_ai_ci',
`IDADE` INT UNSIGNED NULL DEFAULT NULL,
`DATA DE NASCIMENTO` DATE NOT NULL,
`ESCOLARIDADE` SET('ANALFABETO','ENSINO FUNDAMENTAL','ENSINO MEDIO','ENSINO SUPERIOR COMPLETO','ENSINO SUPERIOR INCOMPLETO','POS-GRADUAÇÃO','MESTRADO','DOUTORADO') NOT NULL DEFAULT 'ANALFABETO,ENSINO FUNDAMENTAL,ENSINO MEDIO,ENSINO SUPERIOR COMPLETO,ENSINO SUPERIOR INCOMPLETO,POS-GRADUAÇÃO,MESTRADO,DOUTORADO' COLLATE 'utf8mb4_0900_ai_ci',
`SITUAÇÃO PROFISSIONAL` SET('ESTUDANTE','DESEMPREGADO','EMPREGADO') NOT NULL DEFAULT 'ESTUDANTE,DESEMPREGADO,EMPREGADO' COLLATE 'utf8mb4_0900_ai_ci',
`SALARIO` FLOAT NULL DEFAULT NULL,
`POSSUI FILHOS` SET('SIM','NÃO') NULL DEFAULT 'SIM,NÃO' COLLATE 'utf8mb4_0900_ai_ci',
`QUANTOS FILHOS` INT NULL DEFAULT NULL,
`CANDIDATO A PREFEITO QUE PRETENDE VOTAR` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`CANDIDATO A PREFEITO QUE JAMAIS VOTARIA` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`CANDIDATO A VEREADOR QUE PRETENDE VOTAR` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`CANDIDATO A VEREADOR QUE JAMAIS VOTARIA` VARCHAR(50) NULL DEFAULT NULL COLLATE 'utf8mb4_0900_ai_ci',
`SATISFEITO COM O GOVERNO ATUAL` SET('SIM','NÃO') NOT NULL DEFAULT 'SIM,NÃO' COLLATE 'utf8mb4_0900_ai_ci',
PRIMARY KEY (`NUMERO DE ORDEM`) USING BTREE,
INDEX `DATA DA PESQUISA` (`DATA DA PESQUISA`) USING BTREE
)
COLLATE='utf8mb4_0900_ai_ci'
ENGINE=InnoDB
AUTO_INCREMENT=3
